#include <iostream>
using namespace std;
int main() {
    int n, og, sum = 0;
    cout << "Enter a number: ";
    cin >> n;
    og = n;
    while(num > 0) {
        sum += n % 10;
        n /= 10;
    }
    if(og % sum == 0) cout << "Harshad number";
    else cout << "Not a Harshad number";
    return 0;
}
