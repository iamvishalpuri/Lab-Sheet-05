#include <iostream>
using namespace std;
int main() {
    int n;
    cout << "Enter N: ";
    cin >> n;
    cout << "Prime numbers: ";
    for(int num = 2; num <= n; num++) {
        bool is_prime = true;
        for(int i = 2; i <= num/2; i++) {
            if(num % i == 0) {
                is_prime = false;
                break;
            }
        }
        if(is_prime) cout << num << " ";
    }
    return 0;
}

OUTPUT
Enter N: 20
Prime numbers: 2 3 5 7 11 13 17 19 
